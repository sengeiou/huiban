        <attr name="have_line" format="boolean"/>
        <attr name="line_color" />
        <attr name="middle_show" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="BarsCharView">
        <attr name="title" />
        <attr name="bord" format="boolean" />
        <attr name="title_left" format="string" />
        <attr name="h_w" format="float" />
