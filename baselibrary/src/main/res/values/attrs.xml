<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="title_size" format="dimension" />
    <attr name="title" format="string" />
    <attr name="title_color" format="color" />
    <attr name="right_size" format="dimension"/>
    <attr name="right_color" format="color"/>
    <attr name="start_color" format="color"/>
    <attr name="end_color" format="color"/>
    <attr name="default_color" format="color"/>
    <declare-styleable name="LineTextView">
        <attr name="lineType" format="enum">
            <enum name="left" value="0"/>
            <enum name="bottom" value="1"/>
        </attr>
        <attr name="lineColor" format="color"/>
        <attr name="lineWidth" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="TitleView">
        <attr name="title_text" format="string"/>
        <attr name="title_size" />
