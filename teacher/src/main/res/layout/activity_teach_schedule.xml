<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <com.bshuiban.baselibrary.view.customer.TitleView
            android:id="@+id/titleView"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_40"
            app:title_text="我的课表"
            android:background="@color/guide_start_btn"
            app:src_left="@mipmap/iv_back"
            app:title_color="@android:color/white"
            app:title_size="@dimen/dp_15"/>
        <com.bshuiban.baselibrary.view.customer.ClassSchedule
            android:id="@+id/class_schedule"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            />
    </LinearLayout>

</ScrollView>
